{
  "name": "flog",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "bundle": "rollup -c",
    "watch": "npm-watch",
    "build": "npm run bundle && npm run fonts && npm run sass-prod && bundle exec jekyll build",
    "serve": "npm run bundle && npm run fonts && npm run sass-prod && bundle exec jekyll serve",
    "sass": "sass --watch --update --style=expanded _sass/main.scss:assets/css/main.css",
    "sass-prod": "sass --no-source-map --style=compressed _sass/main.scss:assets/css/main.css",
    "dev": "concurrently \"npm run watch\" \"npm run sass\" \"bundle exec jekyll serve --watch --livereload\"",
    "fonts": "copyfiles -f node_modules/@phosphor-icons/web/src/bold/*.woff node_modules/@phosphor-icons/web/src/bold/*.woff2 node_modules/@phosphor-icons/web/src/bold/*.ttf node_modules/@phosphor-icons/web/src/bold/*.svg assets/css"
  },
  "author": "thefipster",
  "license": "ISC",
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "@phosphor-icons/web": "^2.1.1",
    "chart.js": "^4.4.3",
    "chartist": "^1.3.0",
    "flag-icons": "^7.2.2",
    "glob": "^10.4.1",
    "jquery": "^3.7.1",
    "leaflet": "^1.9.4",
    "npm-watch": "^0.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "rollup": "^4.18.0",
    "sass": "^1.77.2"
  },
  "watch": {
    "bundle": {
      "patterns": [
        "_scripts"
      ],
      "extensions": "js"
    }
  }
}
