{
  "name": "flog",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "bundle": "rollup -c",
    "watch": "npm-watch",
    "build": "npm run bundle && bundle exec jekyll build",
    "serve": "npm run bundle && bundle exec jekyll serve",
    "dev": "concurrently \"npm run watch\" \"bundle exec jekyll serve --watch\""
  },
  "author": "thefipster",
  "license": "ISC",
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "chart.js": "^4.4.3",
    "chartist": "^1.3.0",
    "glob": "^10.4.1",
    "jquery": "^3.7.1",
    "leaflet": "^1.9.4",
    "npm-watch": "^0.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "concurrently": "^8.2.2",
    "rollup": "^4.18.0"
  },
  "watch": {
    "build": {
      "patterns": [
        "_scripts"
      ],
      "extensions": "js"
    }
  }
}
